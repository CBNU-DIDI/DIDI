<div class="modal fade " id="roomModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-xl modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="exampleModalLabel">투표방 정보</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div><h4>후보자</h4></div>
        <% for(let i=0;i<2;i++){%>
          <div class="row" id="r<%=i%>">
          <%let multiple=i*4;%>
          <%for(let j=1;j<5;j++){%>
            <% const num=multiple+j;%>
            <div class="col-md-3" id="c<%=num%>">
              <img src="" id="image<%=num%>" class="card-img-top" onerror="handleError(this)">
              <div class="card-body">
                  <ul class="list-group list-group-flush">
                      <li class="list-group-item" id="name<%=num%>"></li>
                      <li class="list-group-item" id="age<%=num%>"></li>
                  </ul>
              </div>
            </div>
          <%}%>
          </div>
        <%}%>
        <div><h4>투표방설명</h4></div>
          <div class="row">
            <div id="desc"></div>
          </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>